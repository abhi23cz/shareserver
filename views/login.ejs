<form action="/login" method="post">

    
  <section class="text-gray-600 body-font">
    <div class="container px-5 py-24 mx-auto flex flex-wrap items-center">
      <div class="lg:w-3/5 md:w-1/2 md:pr-16 lg:pr-0 pr-0">
        <div class="sm:w-1/3 text-center sm:pr-8 sm:py-8">
        
            <div class="w-20 h-20 rounded-full inline-flex items-center justify-center bg-gray-200 text-gray-400">
                <img alt="content" class="object-cover object-center h-half w-half" src="logo.jpg">
            </div>
        </div>
        <h1 class="title-font font-medium text-3xl text-gray-900">Welcome To The Tiger Server !</h1>
        <p class="leading-relaxed mt-4">Here you can upload your files and download it without Internet.</p>
      </div>
      <div class="lg:w-2/6 md:w-1/2 bg-gray-100 rounded-lg p-8 flex flex-col md:ml-auto w-full mt-10 md:mt-0">
